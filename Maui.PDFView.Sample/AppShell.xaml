<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui.PDFView.Sample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Maui.PDFView.Sample.Pages"
    Title="Maui.PDFView Sample" >

   <ShellContent
        Title="Sample"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

   <ShellContent
        Title="Legacy"
        ContentTemplate="{DataTemplate local:LegacyMainPage}"
        Route="Leagacy" />

</Shell>
